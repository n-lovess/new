CHIP P2S {
    IN indata[4], enable, load;
    OUT sout, complete;

    PARTS:
    Register(in=indata, load=load, out=storedData[4]); 

    Counter(inc=enable, reset=load, a=count0, b=count1, c=false, d=false, e=false, f=false, g=false); 

    Mux4Way16(a=storedData[0], b=storedData[1], c=storedData[2], d=storedData[3], sel[0..1]={count1, count0}, out=sout);

    And(a=count0, b=count1, out=done);
    And(a=done, b=enable, out=complete);
}