CHIP P2S {
    IN indata[4], enable, load;
    OUT sout, complete;

    PARTS:
    Register(in=indata, load=load, out=storedData);

    Counter(inc=enable, reset=load, out=count[2]);

    Mux4Way16(a=storedData[0], b=storedData[1], c=storedData[2], d=storedData[3], sel=count, out=sout);

    And(a=count[1], b=count[0], out=done);
    And(a=done, b=enable, out=complete);
}