CHIP DecoderSegment201830364 {
    IN A, B, C, D; 
    OUT d;         

    PARTS:
    Not(in=A, out=notA);
    Not(in=B, out=notB);
    Not(in=C, out=notC);
    Not(in=D, out=notD);

    And(a=notA, b=notB, c=C, d=notD, out=term1);
    And(a=notA, b=B, c=notC, d=D, out=term2);
    And(a=A, b=notB, c=C, d=D, out=term3);
    And(a=A, b=B, c=notC, d=notD, out=term4);

    Or(a=term1, b=term2, out=or1);
    Or(a=term3, b=term4, out=or2);
    Or(a=or1, b=or2, out=d);
}

