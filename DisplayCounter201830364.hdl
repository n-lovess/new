CHIP DisplayCounter201830364 {
    IN inc, reset;
    OUT a,b,c,d,e,f,g;

    PARTS:
    Inc16(in=counterOut, out=incremented);
    Mux16(a=incremented, b=false, sel=reset, out=nextValue);
    Register(in=nextValue, load=inc, out=counterOut);

    Splitter(in=counterOut, out[0]=bit0, out[1]=bit1, out[2]=bit2, out[3]=bit3);

    Decoder(A=bit0, B=bit1, C=bit2, D=bit3, a=a, b=b, c=c, d=d, e=e, f=f, g=g);
}