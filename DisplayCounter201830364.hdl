CHIP DisplayCounter201830364 {
    IN inc, reset;
    OUT a,b,c,d,e,f,g;

    PARTS:
    Inc16(in=counterOut, out=incremented);
    Mux16(a=incremented, b=false, sel=reset, out=nextValue);
    Register(in=nextValue, load=inc, out=counterOut);

    Or8Way(in=counterOut[0..7], out=bit0);
    Or8Way(in=counterOut[8..15], out=bit1);

    Decoder(A=counterOut[0], B=counterOut[1], C=counterOut[2], D=counterOut[3], a=a, b=b, c=c, d=d, e=e, f=f, g=g);
}