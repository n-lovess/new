CHIP DecoderSegment201830364 {  
    IN A, B, C, D;  
    OUT d;  
    
    PARTS:
    NOT(a=A, out=A_);  // A'
    AND(a=A_, b=B, out=temp1);  // A'B
    AND(a=A, b=C, out=temp2);  // AC
    NOT(a=D, out=D_);  // D'
    AND(a=B, b=D_, out=temp3);  // BD'
    OR(a=temp1, b=temp2, out=out1);  // A'B + AC
    OR(a=out1, b=temp3, out=d);  // A'B + AC + BD'
}